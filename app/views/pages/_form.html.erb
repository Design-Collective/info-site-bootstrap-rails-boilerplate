<%= simple_form_for :page, :defaults => { :input_html => { :class => "form-control" } } do |f| %>
  <%= f.error_notification %>
    <%= f.input :meta_title %>
    <%= f.input :meta_description %>
    <%= f.input :page_title %>
    <%= f.input :header_image %>
    <%= f.input :body %>
    <%= f.button :submit %>
<% end %>
